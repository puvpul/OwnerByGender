@model IEnumerable<PetOwnerGender.Models.PetOwnerVM>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
  
   
        @foreach (PetOwnerGender.Models.PetOwnerVM vm in Model)
        {
            <p>@vm.gender</p>
            foreach (PetOwnerGender.Models.Pet p in vm.pets)
            {

                <li style="margin-left: 20px">@p.name</li>
            }
        }
    
</div>